<h1>Create a New Condition</h1>
<form action="/conditions" method="POST">
  <input class="edit_or_new_form" type="datetime" name="condition[date]" placeholder="date"><br>
  <input class="edit_or_new_form" type="float" name="condition[max_temperature]" placeholder="max_temperature"><br>
  <input class="edit_or_new_form" type="float" name="condition[mean_temperature]" placeholder="mean_temperature"><br>
  <input class="edit_or_new_form" type="float" name="condition[min_temperature]" placeholder="min_temperature"><br>
  <input class="edit_or_new_form" type="float" name="condition[mean_humidity]" placeholder="mean_humidity"><br>
  <input class="edit_or_new_form" type="float" name="condition[mean_visibility]" placeholder="mean_visibility"><br>
  <input class="edit_or_new_form" type="float" name="condition[max_wind_speed]" placeholder="max_wind_speed"><br>
  <input class="edit_or_new_form" type="float" name="condition[]"
  <input type="submit" value="Create Condition">
</form>
